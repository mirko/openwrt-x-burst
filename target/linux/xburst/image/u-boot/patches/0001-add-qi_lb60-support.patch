+
+#if defined(CONFIG_MIPS_JZ4730_PMPV1) || defined(CONFIG_MIPS_JZ4730_PMPV2)
+	#define SPEN	60       /*LCD_SPL */
+	#define SPCK	61       /*LCD_CLS */
