+unsigned long
+calculate_board_ddr_clk(ulong dummy)
+{
+	ulong val;
+	val = ics307_clk_freq(
+	    in8(PIXIS_BASE + PIXIS_VDDRCLK0),
+	    in8(PIXIS_BASE + PIXIS_VDDRCLK1),
+	    in8(PIXIS_BASE + PIXIS_VDDRCLK2));
+	debug("ddrclk val = %lu\n", val);
+	return val;
+}
